.list-agents
  *
    outline none !important

  .che-list
    margin-bottom 0
    background-color inherit

  .che-list-header-content > *, .che-list
    margin 0

  .che-list-header md-item
    border-top none

  .che-list-item-content *:not(.tooltip)
    overflow visible

  .agent-item-row
    min-height 33px
    .agent-description
      max-width 50%
      overflow hidden
